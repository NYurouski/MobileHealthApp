<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
xmlns="http://xamarin.com/schemas/2014/forms"
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
BackgroundColor="#ffffff"
x:Class="oc.Source.Pages.ViewHelpers.SubEventsView">
	<ContentView.Padding>
		<OnPlatform
				x:TypeArguments="Thickness"
				iOS="0, 20, 0, 0"
				Android="0, 0, 0, 0"
				WinPhone="0, 0, 0, 0" />

	</ContentView.Padding>
	<ContentView.Content>
		<ScrollView>
			<StackLayout BackgroundColor="#ffffff"  IsVisible="False" x:Name="SheduleLayout">
				<StackLayout.Padding>
					<OnPlatform
					  x:TypeArguments="Thickness"
					  iOS="0, 0, 0, 0"/>
				</StackLayout.Padding>
			</StackLayout>
		</ScrollView>
	</ContentView.Content>
</ContentView>
